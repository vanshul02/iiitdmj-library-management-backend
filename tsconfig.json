{
  "extends": "./node_modules/gts/tsconfig-google.json",
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "rootDir": "."
   },
   "include": [
     "src/**/*.ts",
     "test/**/*.ts"
   ]
}